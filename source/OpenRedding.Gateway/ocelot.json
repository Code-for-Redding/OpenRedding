{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/salaries",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "open_redding_api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/v1/salaries",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/salaries?name={name}&jobTitle={jobTitle}&agency={agency}&status={status}&sortBy={sortBy}&sortField={sortField}&page={page}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "open_redding_api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/v1/salaries?name={name}&jobTitle={jobTitle}&agency={agency}&status={status}&sortBy={sortBy}&sortField={sortField}&page={page}",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/salaries/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "open_redding_api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/v1/salaries/{id}",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/salaries/download",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "open_redding_api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/v1/salaries/download",
      "UpstreamHttpMethod": [ "Post" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:6001"
  }
}
