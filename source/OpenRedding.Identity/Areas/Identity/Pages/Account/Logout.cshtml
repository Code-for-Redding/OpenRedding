@page
@model Logout
@{
    ViewData["Title"] = "Log out";
}

<div class="row align-self-center w-100">
    <div class="col-6 mx-auto">
        <div class="card">
            <div class="card-body">
                <h4 class="text-center">See you later!</h4>
                <p>You have been successfully logged out.</p>
            </div>
            <div class="card-footer">
                <div class="d-flex">
                    <div class="mr-auto">
                        <a asp-area="Identity" asp-page="/About" class="px-2">About</a>
                        <a asp-area="Identity" asp-page="/Account/Login" class="px-2">Login</a>
                        <a asp-area="Identity" asp-page="/Account/Register">Register</a>
                    </div>
                    <i onclick="window.open('https://github.com/joey32793/OpenRedding')" class="fab fa-github fa-2x px-2"></i>
                    <i onclick="window.open('https://registry.jsonresume.org/joey32793')" class="fas fa-code fa-2x px-2"></i>
                </div>
            </div>
        </div>
    </div>
</div>

